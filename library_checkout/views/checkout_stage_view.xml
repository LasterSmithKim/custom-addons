<?xml version="1.0" ?>
<odoo>
    <record id="view_tree_checkout_stage" model="ir.ui.view">
        <field name="name">Checkout_Stage Tree</field>
        <field name="model">library.checkout.stage</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name" />
                <field name="sequence" />
                <field name="fold" />
                <field name="active" />
                <field name="state" />

            </tree>
        </field>
    </record>

    <record id="view_form_checkout_stage" model="ir.ui.view">
        <field name="name">Checkout_Stage Form</field>
        <field name="model">library.checkout.stage</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name="name" />
                        <field name="sequence" />
                        <field name="fold" />
                        <field name="active" />
                        <field name="state" />
                    </group>
                </sheet>
            </form>
        </field>
    </record>
</odoo>